local function UKEC_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local frames = 0
	
	game:GetService("RunService").RenderStepped:Connect(function()
		frames += 1
	end)
	
	while wait(1) do
		script.Parent.Text = "FPS: " .. frames
		frames = 0
	end
end
coroutine.wrap(UKEC_fake_script)()
local function DPCVG_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(DPCVG_fake_script)()
local function WWUQ_fake_script() -- Open.LocalScript 
