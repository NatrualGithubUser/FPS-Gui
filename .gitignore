-- Scripts:

local function WSDJP_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local frames = 0
	
	game:GetService("RunService").RenderStepped:Connect(function()
		frames += 1
	end)
	
	while wait(1) do
		script.Parent.Text = "FPS: " .. frames
		frames = 0
	end
end
coroutine.wrap(WSDJP_fake_script)()
